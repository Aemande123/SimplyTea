{
  "multipart": [
    { "apply": { "model": "simplytea:block/fence/post" }},
    { "when": { "north": "true" },
      "apply": { "model": "simplytea:block/fence/side" }
    },
    { "when": { "east": "true" },
      "apply": { "model": "simplytea:block/fence/side", "y": 90 }
    },
    { "when": { "south": "true" },
      "apply": { "model": "simplytea:block/fence/side", "y": 180 }
    },
    { "when": { "west": "true" },
      "apply": { "model": "simplytea:block/fence/side", "y": 270 }
    }
  ]
}
